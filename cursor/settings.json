{
    "workbench.colorTheme": "Ros√© Pine (no italics)",
    "editor.fontFamily": "'Fira Code Nerd Font'",
    // "terminal.integrated.fontFamily": "monospace",
    // "custom-ui-style.font.monospace": "Fira Code Nerd Font",
    // "custom-ui-style.font.sansSerif": "Fira Code Nerd Font",
    // "editor.codeLensFontFamily": "Fira Code Nerd Font",
    // "editor.inlayHints.fontFamily": "Fira Code Nerd Font",
    // "editor.inlineSuggest.fontFamily": "Fira Code Nerd Font",
    // "debug.console.fontFamily": "Fira Code Nerd Font",
    // "scm.inputFontFamily": "Fira Code Nerd Font",
    // "notebook.output.fontFamily": "Fira Code Nerd Font",
    // "gitlens.blame.fontFamily": "Fira Code Nerd Font",
    "settings.cycle": [
        {
            "id": "relativeLineNumbers",
            "overrideWorkspaceSettings": false,
            "values": [
                {
                    "editor.lineNumbers": "relative"
                },
                {
                    "editor.lineNumbers": "on"
                }
            ]
        },
        {
            "id": "tabBar",
            "overrideWorkspaceSettings": false,
            "values": [
                {
                    "workbench.editor.showTabs": "none"
                },
                {
                    "workbench.editor.showTabs": "multiple"
                }
            ]
        }
    ],
    "editor.lineNumbers": "relative",
    "editor.fontSize": 13,
    "terminal.integrated.fontSize": 13,
    "custom-ui-style.electron": {
        "transparent": true,
        "opacity": 0.95,
        "frame": false,
        "vibrancy": "ultra-dark",
        "visualEffectState": "active"
    },
    "workbench.activityBar.location": "hidden",
    "editor.fontWeight": 100,
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "workbench.iconTheme": "material-icon-theme",
    "window.zoomLevel": 0.25,
    "workbench.editor.showTabs": "multiple",
    "workbench.editor.editorActionsLocation": "hidden",
    "workbench.layoutControl.enabled": false,
}
